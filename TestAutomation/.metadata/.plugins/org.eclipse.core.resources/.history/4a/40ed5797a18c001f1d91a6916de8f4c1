package Objects;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;

public class TerminierungsportalFunktionen {

	WebDriver driver;
	public TerminierungsportalFunktionen(WebDriver driver) {
		this.driver = driver;
	}
	String URL="http://localhost:8080/";
	
	By userName = By.xpath("//input[@id='email']");
	By pwd = By.xpath("//input[@id='pass']");
	By loginBtn = By.xpath("//button[@name='login']");
	
	public void OpenFacebookURL() {
		try {
			driver.get(URL);
			driver.findElement(By.xpath("(//div[@role='none'])[27]")).click();
			Thread.sleep(5000);
		}
		catch(Exception e) {
			System.out.println("Exceptions Caught: "+e.getMessage());
		}		
	}
}
